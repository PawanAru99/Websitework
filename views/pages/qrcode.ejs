                <div class="btn-group">
                    <button type="button" class="btn btn-primary" onClick="showImage('first')">Washim Main Branch</button>
                    <button type="button" class="btn btn-primary" onClick="showImage('second')">Washim Branch</button>
                    <button type="button" class="btn btn-primary" onClick="showImage('third')">Risod Branch</button>
                    <button type="button" class="btn btn-primary" onClick="showImage('four')">Mangulphir Branch</button>
                    <button type="button" class="btn btn-primary" onClick="showImage('five')">Manora Branch</button>
